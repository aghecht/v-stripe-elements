{ /* TypeScript Options for Distribution */
  "extends": "./tsconfig.json", /* overrides/extends the Base options */
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "outDir": "./lib-temp", /* intermediate directory from which dist is built */
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true
  }
}
